{
  "name": "pst",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "start": "npm run env -s && ng serve --aot  --proxy-config proxy.conf.js --host 0.0.0.0 --port 80",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^6.1.7",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.7",
    "@angular/compiler": "^6.1.7",
    "@angular/core": "^6.1.7",
    "@angular/flex-layout": "6.0.0-beta.18",
    "@angular/forms": "^6.1.7",
    "@angular/http": "^6.1.7",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.7",
    "@angular/platform-browser-dynamic": "^6.1.7",
    "@angular/router": "^6.1.7",
    "@angular/service-worker": "^6.1.7",
    "@ng-select/ng-select": "^2.9.0",
    "@ngx-translate/core": "^10.0.1",
    "@types/luxon": "^1.4.0",
    "@types/moment": "^2.13.0",
    "@types/numeral": "0.0.25",
    "angular-calendar": "^0.26.3",
    "bootstrap": "^4.1.3",
    "class-transformer": "^0.1.10",
    "core-js": "^2.5.6",
    "date-fns": "^1.29.0",
    "fullcalendar": "^4.0.0-alpha.2",
    "g": "^2.0.1",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.10",
    "material-design-icons-iconfont": "^3.0.3",
    "moment": "^2.22.2",
    "ngx-bootstrap": "^3.0.1",
    "ngx-mat-select-search": "^1.4.1",
    "ngx-perfect-scrollbar": "^6.3.1",
    "numeral": "^2.0.6",
    "primeicons": "^1.0.0-beta.10",
    "primeng": "^6.1.4",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.1",
    "typeorm": "^0.2.7",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.1",
    "@angular/cli": "^6.2.4",
    "@angular/compiler-cli": "^6.1.7",
    "@angular/language-service": "^6.1.7",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@ngx-rocket/scripts": "^3.0.0",
    "@types/jasmine": "^2.8.7",
    "@types/jasminewd2": "^2.0.2",
    "@types/lodash": "^4.14.108",
    "@types/node": "^8.10.13",
    "codelyzer": "^4.4.4",
    "hads": "^1.7.0",
    "htmlhint": "^0.10.0",
    "https-proxy-agent": "^2.0.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.2",
    "karma-jasmine": "^1.1.2",
    "karma-junit-reporter": "^1.2.0",
    "protractor": "~5.4.0",
    "puppeteer": "^1.4.0",
    "stylelint": "~9.5.0",
    "stylelint-config-recommended-scss": "~3.2.0",
    "stylelint-config-standard": "~18.2.0",
    "stylelint-scss": "~3.3.0",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  }
}
